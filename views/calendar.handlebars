<script>
    $("#add").click(function(){
        $(".myPatient").toggle();
    });
    $("#update").click(function(){
        $(".myUpdate").toggle();
    });
    var superdata=[];
     $.get("http://localhost:2000/calendar/appointment", function( data ) {
         for(var x = 0;x<data.length;x++){
            superdata.push(data[x]);
        }
        console.log(superdata.length);
        var date = new Date().toLocaleDateString()
        var totalApp = 0;
        var counter = 0;
        for(var i=0;i<data.length;i++){
            for(var j = 0;j<data[i].time.length;j++){
                if(data[i].time[j].date === date){
                    totalApp += 1;
                    counter++
                    $("#allAppointments").append("<tr>"+ "<th scope='row' class='num'>"+counter+"</th>" +
                    "<td>"+data[i]._id+"</td>" + "<td>"+data[i].time[j].date +"</td>" + "<td>" + 
                    data[i].time[j].hour +"</td></tr>");   
                }                     
            }
        }      
        $(".appointments").text(totalApp); 
        
     });
   
     $("#seeApp").click(function(){
            var patient = "";
            var p = document.getElementById("appPatient");
            patient = p.options[p.selectedIndex].text;
            var index=-1;
            var length = superdata.length;
            console.log("length " +length);
            var flag = false;
            for(var i = 0;i<superdata.length;i++){
               if(patient === superdata[i]._id){
                    console.log(i);
                    index = i;
                    flag = true;
                }
            }
            if(flag === true){
                $("#HAppointments td").remove();
                $(".num").remove();  
               for(var j = 0;j<superdata[index].time.length;j++){
                    $("#HAppointments").append("<tr>"+ "<th scope='row' class='num'>"+j+"</th>" +
                    "<td>"+superdata[index].time[j].date +"</td>" + "<td>" + superdata[index].time[j].hour +"</td></tr>");   
                } 
            }
            else{
                $("#HAppointments td").remove();
                $(".num").remove();  
            }
            
    }); //dateApp
    $("#seeHapp").click(function(){
            var totalApp = 0;
            var counter = 0;
            var date = $("#dateApp").val();
            console.log(date);
            for(var i=0;i<superdata.length;i++){
                for(var j = 0;j<superdata[i].time.length;j++){
                    if(superdata[i].time[j].date === date){
                        totalApp += 1;
                        counter++
                        $("#Happointments").append("<tr>"+ "<th scope='row' class='num'>"+counter+"</th>" +
                        "<td>"+superdata[i]._id+"</td>" + "<td>"+superdata[i].time[j].date +"</td>" + "<td>" + 
                        superdata[i].time[j].hour +"</td></tr>");   
                    }
                    else{
                        console.log("no matches");
                    }                     
                }
        }      
            
    });
    // http://www.coderanch.com/t/520876/java/java/count-instances-word-String 
    //http://stackoverflow.com/questions/1129216/sort-array-of-objects-by-string-property-value-in-javascript    
</script>

    